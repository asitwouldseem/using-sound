{
    "name": "asitwouldseem/using-sound",
    "type": "project",
    "license": "proprietary",
    "description": "An interactive website created for my 'Future Design Leadership' project on sonic experience and education.",
    "homepage": "https://usingsound.cameron-morgan.com",
    "authors": [
      {
        "name": "Cameron Morgan",
        "email": "hello@cameron-morgan.com",
        "homepage": "https://cameron-morgan.com"
      }
    ],
    "repositories": [
      { "type": "composer", "url": "https://repo.repman.io" },
      { "packagist": false }
    ],
    "require": {
      "craftcms/cms": "^3.5.10",
      "vlucas/phpdotenv": "^3.4.0"
    },
    "require-dev": {
      "yiisoft/yii2-shell": "^2.0.3"
    },
    "autoload": {
      "psr-4": {
        "modules\\": "modules/"
      }
    },
    "config": {
      "sort-packages": true,
      "optimize-autoloader": true,
      "platform": {
        "php": "7.0"
      }
    },
    "scripts": {
      "post-root-package-install": [
        "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
      ]
    }
}
